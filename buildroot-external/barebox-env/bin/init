#!/bin/sh

export PATH=/env/bin

global linux.bootargs.base
global linux.bootargs.dyn.root

# Init board specific stuff
[ -e /env/config-board ] && /env/config-board

# Autostart
for i in /env/init/*; do
	. $i
done

echo "- Hit m for menu or wait for autoboot -"
timeout -a 1 -v key

# Run menu
if [ "${key}" != "m" ]; then
    boot
fi

menutree
